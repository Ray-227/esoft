  "dependencies": {
    "bcryptjs": "^2.4.3",
    "config": "^3.3.6",
    "express": "^4.17.1",
    "express-validator": "^6.11.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.12.12",
    "shortid": "^2.2.16"
  },
  "devDependencies": {
    "concurrently": "^6.2.0",
    "cross-env": "^7.0.3",
